            Sidebar end
        ***********************************-->

        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">
            <div class="container-fluid">
                <div class="row page-titles mx-0">
                    <div class="col-sm-6 p-md-0">
                        <div class="welcome-text">
                            <h4>Tableau de personnes: </h4>
                        </div>
                    </div>
                    <div class="col-sm-6 p-md-0 justify-content-sm-end mt-2 mt-sm-0 d-flex">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="javascript:void(0)">Table</a></li>
                            <li class="breadcrumb-item active"><a href="javascript:void(0)">Bootstrap</a></li>
                        </ol>
                    </div>
                </div>
                <!-- row -->

                <div class="row">
                    
                    
                    
                    
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Table d'utilisateur</h4>
                                <button type="button"  onclick="window.location.href='{{ path('app_user_new') }}'" class="btn btn-primary" >+</button>
                            </div>
                            
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-bordered verticle-middle table-responsive-sm">
                                        <thead class="thead-primary">
                                            <tr>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Email</th>
                <th>Number</th>
                <th>Role</th>
                <th>actions</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        {% for user in users %}
                                            <tr>
                                                <td>{{user.nom}}</td>
                                                <td>{{user.prenom}}</td>
                                                <td>{{user.email}}</td>
                                                <td>{{user.number}}</td>
                                                <td>{{user.role}}</td>
                                                <td>
                                                    <span>
                                                        <a href="{{ path('app_user_edit', {'iduser': user.iduser}) }}" class="mr-4" data-toggle="tooltip"
                                                            data-placement="top" title="Edit"><i
                                                                class="fa fa-pencil color-muted"></i> </a>
                                                                {{ include('templates_Back/user/_delete_form.html.twig', {'button_label': 'Supprimer'}) }}   
                                                        
                                                    </span>
                                                </td>
                                            </tr>
                                             {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                   
                    
                   
                   
                </div>
            </div>
        </div>
        <!--**********************************
            Content body end
        ***********************************-->


